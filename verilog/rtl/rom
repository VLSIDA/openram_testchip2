#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5598e1a252d0 .scope module, "testbench" "testbench" 2 2;
 .timescale -9 -12;
P_0x5598e1a23190 .param/l "ADDR_WIDTH" 1 2 6, +C4<00000000000000000000000000001010>;
P_0x5598e1a231d0 .param/l "DATA_WIDTH" 1 2 5, +C4<00000000000000000000000000001000>;
P_0x5598e1a23210 .param/l "ROM_DEPTH" 1 2 7, +C4<000000000000000000000000000000010000000000>;
P_0x5598e1a23250 .param/l "WAVEFORM" 1 2 4, +C4<00000000000000000000000000000001>;
v0x5598e1a54930_0 .var "addr", 9 0;
v0x5598e1a54a10_0 .var "clk", 0 0;
v0x5598e1a54ae0_0 .var "cs", 0 0;
v0x5598e1a54be0_0 .net "dout", 7 0, v0x5598e1a546c0_0;  1 drivers
v0x5598e1a54cb0 .array "mem", 1023 0, 7 0;
E_0x5598e1a3c150 .event negedge, v0x5598e1a27240_0;
S_0x5598e1a29120 .scope module, "rom_inst" "sky130_rom_1kbyte_8x1024" 2 18, 3 8 0, S_0x5598e1a252d0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "cs";
    .port_info 2 /INPUT 10 "addr";
    .port_info 3 /OUTPUT 8 "dout";
P_0x5598e19facf0 .param/l "ADDR_WIDTH" 0 3 18, +C4<00000000000000000000000000001010>;
P_0x5598e19fad30 .param/l "DATA_WIDTH" 0 3 17, +C4<00000000000000000000000000001000>;
P_0x5598e19fad70 .param/l "DELAY" 0 3 21, +C4<00000000000000000000000000000011>;
P_0x5598e19fadb0 .param/l "ROM_DEPTH" 0 3 19, +C4<000000000000000000000000000000010000000000>;
P_0x5598e19fadf0 .param/l "T_HOLD" 0 3 23, +C4<00000000000000000000000000000001>;
P_0x5598e19fae30 .param/l "VERBOSE" 0 3 22, +C4<00000000000000000000000000000001>;
v0x5598e1a26e40_0 .net "addr", 9 0, v0x5598e1a54930_0;  1 drivers
v0x5598e1a27240_0 .net "clk", 0 0, v0x5598e1a54a10_0;  1 drivers
v0x5598e1a545f0_0 .net "cs", 0 0, v0x5598e1a54ae0_0;  1 drivers
v0x5598e1a546c0_0 .var "dout", 7 0;
v0x5598e1a547a0 .array "mem", 1023 0, 7 0;
E_0x5598e1a280e0 .event posedge, v0x5598e1a27240_0;
S_0x5598e1a39d90 .scope begin, "MEM_READ0" "MEM_READ0" 3 45, 3 45 0, S_0x5598e1a29120;
 .timescale -9 -12;
    .scope S_0x5598e1a29120;
T_0 ;
    %vpi_call 3 37 "$readmemh", "./sky130_rom_1kbyte_8x1024.hex", v0x5598e1a547a0, 32'sb00000000000000000000000000000000, 42'sb000000000000000000000000000000001111111111 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x5598e1a29120;
T_1 ;
    %wait E_0x5598e1a280e0;
    %fork t_1, S_0x5598e1a39d90;
    %jmp t_0;
    .scope S_0x5598e1a39d90;
t_1 ;
    %delay 1000, 0;
    %pushi/vec4 255, 255, 8;
    %assign/vec4 v0x5598e1a546c0_0, 0;
    %load/vec4 v0x5598e1a545f0_0;
    %pushi/vec4 1, 0, 1;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x5598e1a26e40_0;
    %pad/u 12;
    %ix/vec4 4;
    %load/vec4a v0x5598e1a547a0, 4;
    %vpi_call 3 48 "$display", $time, " Reading %m addr=%b dout=%b", v0x5598e1a26e40_0, S<0,vec4,u8> {1 0 0};
T_1.0 ;
    %load/vec4 v0x5598e1a545f0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %delay 1000, 0;
    %load/vec4 v0x5598e1a26e40_0;
    %pad/u 12;
    %ix/vec4 4;
    %load/vec4a v0x5598e1a547a0, 4;
    %assign/vec4 v0x5598e1a546c0_0, 3000;
T_1.2 ;
    %end;
    .scope S_0x5598e1a29120;
t_0 %join;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5598e1a252d0;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5598e1a54a10_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5598e1a54a10_0;
    %inv;
    %store/vec4 v0x5598e1a54a10_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x5598e1a252d0;
T_3 ;
    %vpi_call 2 38 "$dumpfile", "iverilog.vcd" {0 0 0};
    %vpi_call 2 40 "$dumpvars" {0 0 0};
    %vpi_call 2 43 "$display", "Begin Test:" {0 0 0};
    %vpi_call 2 45 "$display", "   Reading data file: sky130_rom_1kbyte_8x1024.hex" {0 0 0};
    %vpi_call 2 46 "$readmemh", "sky130_rom_1kbyte_8x1024.hex", v0x5598e1a54cb0 {0 0 0};
    %wait E_0x5598e1a3c150;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5598e1a54ae0_0, 0, 1;
    %pushi/vec4 0, 0, 10;
    %store/vec4 v0x5598e1a54930_0, 0, 10;
    %wait E_0x5598e1a3c150;
    %delay 1000, 0;
T_3.0 ;
    %load/vec4 v0x5598e1a54930_0;
    %pad/u 32;
    %cmpi/u 9, 0, 32;
    %jmp/0xz T_3.1, 5;
    %load/vec4 v0x5598e1a54930_0;
    %pad/u 12;
    %ix/vec4 4;
    %load/vec4a v0x5598e1a54cb0, 4;
    %load/vec4 v0x5598e1a54be0_0;
    %cmp/ne;
    %jmp/0xz  T_3.2, 4;
    %load/vec4 v0x5598e1a54930_0;
    %pad/u 12;
    %ix/vec4 4;
    %load/vec4a v0x5598e1a54cb0, 4;
    %vpi_call 2 57 "$display", "Unexpected value read at address %d. Expected %d, received %d.", v0x5598e1a54930_0, S<0,vec4,u8>, v0x5598e1a54be0_0 {1 0 0};
    %vpi_call 2 58 "$finish" {0 0 0};
T_3.2 ;
    %load/vec4 v0x5598e1a54930_0;
    %addi 1, 0, 10;
    %store/vec4 v0x5598e1a54930_0, 0, 10;
    %wait E_0x5598e1a3c150;
    %delay 1000, 0;
    %jmp T_3.0;
T_3.1 ;
    %load/vec4 v0x5598e1a54930_0;
    %pad/u 12;
    %ix/vec4 4;
    %load/vec4a v0x5598e1a54cb0, 4;
    %load/vec4 v0x5598e1a54be0_0;
    %cmp/ne;
    %jmp/0xz  T_3.4, 4;
    %load/vec4 v0x5598e1a54930_0;
    %pad/u 12;
    %ix/vec4 4;
    %load/vec4a v0x5598e1a54cb0, 4;
    %vpi_call 2 66 "$display", "Unexpected value read at address %d. Expected %d, received %d.", v0x5598e1a54930_0, S<0,vec4,u8>, v0x5598e1a54be0_0 {1 0 0};
    %vpi_call 2 67 "$finish" {0 0 0};
T_3.4 ;
    %vpi_call 2 70 "$display", "Test passed." {0 0 0};
    %vpi_call 2 71 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "rom_testbench.v";
    "sky130_rom_1kbyte_8x1024_tapeout.v";
